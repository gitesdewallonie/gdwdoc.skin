<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="GdwDocSkin">

 <object name="gdwdoc_skin_images"
    meta_type="Filesystem Directory View"
    directory="gdwdoc.skin:skins/gdwdoc_skin_images"/>
 <object name="gdwdoc_skin_styles"
    meta_type="Filesystem Directory View"
    directory="gdwdoc.skin:skins/gdwdoc_skin_styles"/>
 <object name="gdwdoc_skin_templates"
    meta_type="Filesystem Directory View"
    directory="gdwdoc.skin:skins/gdwdoc_skin_templates"/>

 <skin-path name="GdwDocSkin" based-on="Plone Default">
  <layer name="gdwdoc_skin_images"
     insert-after="custom"/>
  <layer name="gdwdoc_skin_styles"
     insert-after="gdwdoc_skin_images"/>
  <layer name="gdwdoc_skin_templates"
     insert-after="gdwdoc_skin_styles"/>
 </skin-path>

</object>
